    color: rgb(15, 96, 225);
    border-right:1px solid rgb(15, 96, 225);
}

#enroll_btn:hover {
    /* background-color: rgb(12, 80, 189); */
    color: rgb(12, 80, 189);
}

#find_btn {
    /* background-color: rgb(15, 96, 225); */
