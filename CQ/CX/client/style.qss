#login_btn {
    background-color: rgb(15, 96, 225);
    border-radius: 60px;  
    border: 2px groove rgb(12, 80, 189);
    /* font: 9pt "AcadEref"; */
    border-style: outset;
}

#login_btn:hover {
    background-color: rgb(12, 80, 189);
    border-radius: 60px;  
    border: 2px groove rgb(15, 96, 225);
    /* font: 9pt "AcadEref"; */
    border-style: inset;
}

#user {
    border-radius: 15px; 
}

#user:hover {
    border: 1px solid rgb(15, 96, 225);
    background-color: rgb(225, 231, 241);
}

#enroll_btn {
    /* background-color: rgb(15, 96, 225); */
    /* border-radius: 12px;  */
